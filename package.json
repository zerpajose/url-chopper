    "eslint": "9.12.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.10.0",
    "express": "4.21.0",
