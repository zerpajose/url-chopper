{
  "name": "url-chopper",
  "version": "1.0.0",
  "description": "Url shortener service",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prettify": "prettier --write ./src",
    "start": "pnpm build && node --env-file=.env dist/index.js",
    "dev": "pnpm exec nodemon src/index.ts",
    "gcp-build": "pnpm build",
    "lint": "eslint .",
    "build": "tsc",
    "test": "tsc --noEmit && node --import tsx --test test/**/*.test.ts",
    "coverage": "node --test --experimental-test-coverage"
  },
  "keywords": [
    "url",
    "shortener"
  ],
  "author": "Jose Zerpa",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "9.12.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.10.0",
    "express": "4.21.0",
    "zod": "^3.23.8"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "volta": {
    "node": "22.9.0",
    "npm": "10.9.0"
  }
}
